public class Locators {

	public static void main(String[] args) throws InterruptedException {
		System.setProperty("webdriver.chrome.driver",
				"C:\\Users\\DURAIRAJ\\eclipse-workspace\\lib\\driver\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.navigate().to("https://www.facebook.com/");
		driver.findElement(By.id("email")).sendKeys("9345755970");
		WebElement findElement = driver.findElement(By.name("pass"));
		findElement.sendKeys("Passwork##");
		WebElement findElement2 = driver.findElement(By.xpath("//*[text()='Log in']"));
		findElement2.click();
		WebDriverWait wait=new WebDriverWait(driver,Duration.ofSeconds(30));
		Alert until = wait.until(ExpectedConditions.alertIsPresent());
		until.accept();
}
}

